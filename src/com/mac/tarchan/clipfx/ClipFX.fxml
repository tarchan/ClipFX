                          <children>
                            <Slider fx:id="rotateSlider" max="360.0" showTickLabels="false" showTickMarks="false" HBox.hgrow="ALWAYS">
                              <HBox.margin>
                                <Insets right="10.0" top="3.0" />
                              </HBox.margin>
                            </Slider>
                            <TextField fx:id="rotateField" prefWidth="50.0" promptText="角度" text="" HBox.hgrow="NEVER" />
                          </children>
                          <padding>
                            <Insets left="10.0" right="10.0" top="10.0" />
                          </padding>
                        </HBox>
                      </children>
                      <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      </columnConstraints>
                      <padding>
                        <Insets />
                      </padding>
                      <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      </rowConstraints>
                    </GridPane>
                  </children>
                </AnchorPane>
              </content>
            </TitledPane>
          </panes>
        </Accordion>
        <StackPane prefHeight="150.0" prefWidth="200.0">
          <children>
            <ScrollPane fx:id="scrollPanel" prefHeight="200.0" prefWidth="200.0" StackPane.alignment="TOP_LEFT">
              <content>
                <ImageView fx:id="canvas" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
              </content>
            </ScrollPane>
          </children>
        </StackPane>
      </items>
    </SplitPane>
  </children>
</VBox>
