                            <FXCollections fx:factory="observableArrayList">
                              <String fx:value="アイテム1" />
                              <String fx:value="アイテム2" />
                              <String fx:value="アイテム3" />
                            </FXCollections>
                          </items>
                          <GridPane.margin>
                            <Insets left="20.0" fx:id="x1" />
                          </GridPane.margin>
                        </ComboBox>
                        <CheckBox fx:id="ratioBox" mnemonicParsing="false" onAction="#onRatio" text="縦横比を固定" GridPane.columnIndex="0" GridPane.halignment="LEFT" GridPane.margin="$x1" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
